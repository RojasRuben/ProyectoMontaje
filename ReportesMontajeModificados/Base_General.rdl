  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Isometrico">
            <Value>=Parameters!Isometrico.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

select 'Taller' Origen, A.Isometrico,
Spool,
convert(varchar,A.Junta) Junta,
TipoJunta,
Diámetro,
DiametroMaximo,
DescMat1,
DescMat2,
Revision,
SoldaduraFecha,
PruebaRTNumeroReporte,
PruebaPTNumeroReporte,
TratamientoPWHTNumeroReporte,
TratamientoPWHTGráfica,
TratamientoDurezasNumeroReporte,
PruebaRTPostTTNumeroReporte, 
PruebaPTPostTTNumeroReporte,
B.NumeroDERequisicion RT,
B.FECHAREQUISCION FechaRT,
C.Reporte PWHT,
C.FECHA FechaPWHT
 from MONTAJE_SEGUIMIENTOJUNTAS A 
 LEFT JOIN MONTAJE_RT B ON A.Isometrico=B.ISOMETRICO AND A.Junta=B.Junta
 LEFT JOIN MONTAJE_PWHT C ON A.Isometrico=C.ISOMETRCICO AND A.Junta=C.Junta
where 
	A.ISOMETRICO=@Isometrico  and 
	SoldaduraFecha is not null

union all
  
select 'Planta',
A.ISOMETRICO,
SPOOL1,
NoJunta,
TipoJunta,
q diametro,
'0' diametro,
MATERIAL1,MATERIAL2,
'N/A' revision,
SOLDADURA,
RT,
PT,
PWHT,
'x' TratamientoPWHTGrafica,
'x' TratamientoDurezasNumeroReporte,
RTPOST, 
'x'  pruebaptpostTTNumeroReporte ,
B.NumeroDERequisicion RT,
B.FECHAREQUISCION FechaRT,
C.Reporte PWHT,
C.FECHA FechaPWHT
from MONTAJE_BASETRAZABILIDAD A 
LEFT JOIN MONTAJE_RT B ON A.Isometrico=B.ISOMETRICO AND A.NoJunta=B.Junta
 LEFT JOIN MONTAJE_PWHT C ON A.Isometrico=C.ISOMETRCICO AND A.NoJunta=C.Junta
 where A.ISOMETRICO=@Isometrico and SOLDADURA is not null 
order by Origen, Revision</CommandText>
      </Query>
